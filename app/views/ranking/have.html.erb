<div class="container">
  <div class="container-inner inner-default clearfix">
<h3>Have item ランキングページ</h3>



<% @items.each.with_index(1) do |item, i| %> 
      <div class="item-box">
        <div class="image">
          <div class="img-wrap">
           <%= image_tag(item.large_image , alt: item.title) %>
          </div>
        </div>

        <div class="detail">
          <p class="title">
           <%= i %>. <%= item.title %>
          </p>
        </div>
        <%= render 'items/action' , item: item if logged_in? %>
      </div>
<% end %>
 
  </div>
</div>